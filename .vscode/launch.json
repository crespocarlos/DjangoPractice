{
	
	"version": "0.2.0",
	"configurations": [
		{
			"name": "Django",
			"type": "python",
			"request": "launch",
			"stopOnEntry": false,
			"program": "${workspaceRoot}/manage.py",
			"pythonPath": "E:/Projetos/Python/3 - Django/Scripts/python.exe",
			"args": [
				"runserver",
				"--noreload"
			],
			"debugOptions": [
				"WaitOnAbnormalExit",
				"WaitOnNormalExit",
				"RedirectOutput",
				"DjangoDebugging"
			]
		}
	]
}